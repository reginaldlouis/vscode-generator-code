{
	"name": <%- JSON.stringify(name) %>,
	"displayName": <%- JSON.stringify(displayName) %>,
	"description": <%- JSON.stringify(description) %>,
	"version": "0.0.1",
	"engines": {
		"vscode": <%- JSON.stringify(vsCodeEngine) %>
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/Extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.helloWorld",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"vscode:prepublish": "<%= pkgManager %> run compile",
		"compile": "fable-splitter",
		"watch": "fable-splitter --watch",
		"pretest": "<%= pkgManager %> run compile",
		"test": "node ./out/test/runTest.js",
		"postinstall": "ts2fable node_modules/@types/vscode/index.d.ts src/VSCode.fs"
	},
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/plugin-transform-modules-commonjs": "^7.5.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": <%- JSON.stringify(vsCodeEngine) %>,
		"fable-compiler": "^2.1.12",
		"fable-splitter": "^2.1.10",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"ts2fable": "^0.6.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	}
}
